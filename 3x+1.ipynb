    "#the ultimate Equation 3x+1 !!!!\n",
    "a = int(input());\n",
    "while a != 1:\n",
    "    print(a)\n",
    "    if a % 2 == 0:\n",
    "        a = a // 2\n",
    "    else:\n",
    "        a = (a * 3) + 1\n",
    "\n",
    "print(a)\n"
